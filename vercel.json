{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/recipes",
      "methods": ["GET", "POST", "DELETE"],
      "dest": "/index.js"
    },
    {
      "src": "/",
      "dest": "/public/HomePage.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
